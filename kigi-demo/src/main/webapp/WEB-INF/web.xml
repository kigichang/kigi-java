<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <!-- <filter>
  	<filter-name>TestFilter</filter-name>
  	<filter-class>tw.kigi.test.TestFilter</filter-class>
  	
  </filter>-->
  <filter>
  	<filter-name>PrepareModule</filter-name>
  	<filter-class>tw.kigi.web.filter.PrepareModule</filter-class>
  	<init-param>
   			<param-name>/path1</param-name>
   			<param-value>conf/path1.xml</param-value>
  	</init-param>
  </filter>
  <filter-mapping>
  	<filter-name>PrepareModule</filter-name>
  	<url-pattern>/path1/*</url-pattern>
  </filter-mapping>
  <filter-mapping>
  	<filter-name>PrepareModule</filter-name>
  	<url-pattern>/path2/*</url-pattern>
  </filter-mapping>
  <!-- <filter-mapping>
  	<filter-name>TestFilter</filter-name>
  	<url-pattern>/test</url-pattern>
  </filter-mapping>-->
  
  <servlet>
  	<servlet-name>Servlet1</servlet-name>
  	<servlet-class>tw.kigi.test.Servlet1</servlet-class>

  </servlet>
  <servlet-mapping>
  	<servlet-name>Servlet1</servlet-name>
  	<url-pattern>/servlet1/*</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>Servlet2</servlet-name>
  	<servlet-class>tw.kigi.test.Servlet2</servlet-class>

  </servlet>
  <servlet-mapping>
  	<servlet-name>Servlet2</servlet-name>
  	<url-pattern>/servlet2/*</url-pattern>
  </servlet-mapping>
  
</web-app>
